<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
      <TargetFrameworks>netcoreapp3.1;netcoreapp2.0;net45</TargetFrameworks>
      <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
  
  <ItemGroup>
      <Compile Remove="System.Diagnostics\**\*" />
  </ItemGroup>

  <ItemGroup>
      <None Include="System.Diagnostics\**\*" />
  </ItemGroup>

  <PropertyGroup>
      <NoWarn>1591</NoWarn>
      <RunAnalyzersDuringBuild>false</RunAnalyzersDuringBuild>
  </PropertyGroup>

</Project>
